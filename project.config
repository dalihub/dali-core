[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group dalihub admins
	owner = group user/Francisco Santos (xkpe)
[access "refs/for/refs/*"]
	read = group user/Francisco Santos (xkpe)
	push = group user/Francisco Santos (xkpe)
	label-Code-Review = -2..+2 group dalihub reviewers
	label-Code-Review = -2..+2 group user/Francisco Santos (xkpe)
	label-Code-Review = -1..-1 group dalihub developers
	label-Verified = -1..+1 group dalihub developers
	label-Verified = -1..+1 group user/Francisco Santos (xkpe)
	rebase = group user/Francisco Santos (xkpe)
	removeReviewer = group user/Francisco Santos (xkpe)
	submit = group user/Francisco Santos (xkpe)
[access "refs/heads/*"]
	read = group user/Francisco Santos (xkpe)
	create = group user/Francisco Santos (xkpe)
	push = +force group user/Francisco Santos (xkpe)
	pushMerge = group user/Francisco Santos (xkpe)
[access "refs/tags/*"]
	push = group user/Francisco Santos (xkpe)
	push = +force group user/Francisco Santos (xkpe)
